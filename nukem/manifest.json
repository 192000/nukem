{
  "manifest_version": 2,
  "name": "Nuke'm - Browsing Data Cleaner",
  "description": "Clean up your browsing data easily.",
  "version": "3.3",
  "author": "ryland192000",
  "icons":{
    "16": "assets/newicon.png",
    "48": "assets/newicon2.png",
 	"128": "assets/newicon4.png" 
  },
  "browser_action": {
   "default_icon": "assets/newicon.png"
  },
  "background": {
      "scripts": ["bg.js","an.js"]
  },
  "options_ui": {
      "page": "options.html",
      "chrome_style": true
  },
  "permissions": [
      "browsingData",
      "notifications",
      "tabs",
      "history",
	  "cookies",
	  "http://*/*",
	  "https://*/*"
  ]	
}